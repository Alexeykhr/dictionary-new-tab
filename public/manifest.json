{
  "name": "Dictionary New Tab",
  "version": "1.0",
  "description": "Study Dictionary when you open a new tab",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "static/background.js"
    ]
  },
  "icons": {
    "16": "img/book16.png",
    "48": "img/book48.png",
    "128": "img/book128.png"
  },
  "permissions": [
    "tabs",
    "tts",
    "notifications",
    "contextMenus",
    "*://translate.google.com/*",
    "*://translate.google.com.ru/*",
    "*://translate.google.com.ua/*"
  ],
  "manifest_version": 2
}
