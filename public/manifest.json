{
  "name": "Dictionary New Tab",
  "version": "1.0",
  "description": "Study Dictionary when you open a new tab",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "static/background.js"
    ]
  },
  "icons": {
    "24": "img/icon.png"
  },
  "permissions": [
    "tabs",
    "tts",
    "notifications",
    "alarms",
    "contextMenus",
    "storage",
    "*://translate.google.com/*"
  ],
  "manifest_version": 2
}
